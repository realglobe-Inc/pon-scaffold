'use strict'

module.exports = [
  'NotfoundView',
  'HomeView'
].reduce((buds, name) => [
  ...buds,
  {
    force: false,
    mode: '644',
    tmpl: `.View.jsx.hbs`,
    path: `${name}.jsx`,
    data: { name }
  },
  {
    force: false,
    mode: '644',
    tmpl: `.View.pcss.hbs`,
    path: `${name}.pcss`,
    data: { name }
  }
], [])

if (!module.parent) {
  require('coz').render(__filename)
}